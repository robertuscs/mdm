<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>

        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>

        <!-- title of the document -->
        <documentTitle>USCS_Extent_Report</documentTitle>

        <!-- report name - displayed at top-nav -->
        <!--<reportName>Automation Test Report</reportName>-->

        <!--report headline - display at top-nav, after report headline-->
        <reportHeadline>USCS_Extent_Report</reportHeadline>

        <!-- Global date format override -->
        <!-- defaults YYYY-MM-dd -->
        <dateFormat>YYYY-MM-dd</dateFormat>

        <!-- Global time format override -->
        <!-- defaults HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>
        <!-- location of charts in the test view -->

        <!-- top, bottom -->
        <testViewChartLocation>bottom</testViewChartLocation>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[ $(document).ready(function() {}); ]]>
        </scripts>
        <scripts>
            <![CDATA[$(document).ready(function() {$('.dashboard-view').click;});
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

            ]]>
        </styles>
    </configuration>
</extentreports>
